!if "$(DEBUG)" == ""
DEBUG = NO
!endif

!if "$(THREAD)" == ""
THREAD = YES
!endif

!if "$(MYTH95ROOT)" == ""
MYTH95ROOT = \MythOS95
!endif

INCPATH   = ;..\..\inc;$(MYTH95ROOT)\inc;

#Ŀ
# File specific variables                                                  
#

LIBPATH = ..\..\obj;$(MYTH95ROOT)\lib;

EXE = test

OBJS = sysmain.obj

LIBS = mythos.lib\
bozo.lib\
chronos.lib\
escher.lib\
felix.lib\
gutenbrg.lib\
ivorymfc.lib\
vangogh.lib\
xfile.lib\
winmm.lib

#Ŀ
# First target                                                             
#

all: setstden $(EXE).exe

#Ŀ
# Standard make include                                                    
#

!include $(MYTH95ROOT)\inc\make.inc

#
#
#                                 Rules
#
#

#--- .c, .cpp, .asm setup in standard include ---

#
#
#                              Dependancies
#
#

#Ŀ
# Executable                                                               
#

$(EXE).exe:   $(OBJS)
    $(LINK) $(LINKOPTS) /OUT:$*.exe @<<
    $(OBJS) $(LIBS)
<<

clean:
    del *.obj
    del *.res
    del *.err
    del *.lst
    del *.ilk
    del *.pdb
    del *.vcp

# End of makefile 

