!if "$(DEBUG)" == ""
DEBUG = NO
!endif

!if "$(THREAD)" == ""
THREAD = YES
!endif

!if "$(MYTH95ROOT)" == ""
MYTH95ROOT = \MythOS95
!endif

INCPATH = ;..\..\inc;$(MYTH95ROOT)\inc;

#Ŀ
# File specific variables                                                  
#

LIBPATH = ..\..\obj;$(MYTH95ROOT)\lib;

EXE = test

OBJS = \
bachtest.obj\
resource.res

LIBS = \
ivory.lib\
bach.lib\
xfile.lib\
dsound.lib\
user32.lib\
gdi32.lib\
version.lib

#Ŀ
# First target                                                             
#

all: setstden $(EXE).exe

#Ŀ
# Standard make include                                                    
#

!include $(MYTH95ROOT)\inc\make.inc

#
#
#                                 Rules
#
#

#--- .c, .cpp, .asm setup in standard include ---

#
#
#                              Dependancies
#
#

#Ŀ
# Executable                                                               
#

$(EXE).exe:   $(OBJS)
   $(LINK) $(LINKOPTS) /OUT:$*.exe /subsystem:windows,4 @<<
   $(OBJS) $(LIBS)
<<

clean:
   del *.obj
   del *.res
   del *.err
   del *.lst
   del *.ilk
   del *.pdb
   del *.pch

# End of makefile 

