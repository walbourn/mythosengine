$(LIB).pch :
    del espch.obj

espch.obj : ..\espch.cpp
    $(CPP) /Ycescher.hpp /Fp$(LIB).pch $(CCOPTIONS) ..\$*.cpp

#ִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִִ¿
