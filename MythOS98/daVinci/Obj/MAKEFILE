!if "$(DEBUG)" == ""
DEBUG = NO
!endif

!if "$(THREAD)" == ""
THREAD = YES
!endif

!if "$(MYTH95ROOT)" == ""
MYTH95ROOT = \MythOS95
!endif

!if "$(MYTH98ROOT)" == ""
MYTH98ROOT = \MythOS98
!endif

INCPATH = ..\inc;$(MYTH98ROOT)\inc;$(MYTH95ROOT)\inc

#Ŀ
# File specific variables                                                  
#

LIB = davinci

OBJS =

#Ŀ
# First target                                                             
#

all: setstden $(LIB).lib

#Ŀ
# Standard make include                                                    
#

!include $(MYTH98ROOT)\inc\make.inc

#
#
#                                 Rules
#
#

#--- .c, .cpp, .asm setup in standard include ---

#
#
#                              Dependancies
#
#

$(LIB).lib:   $(OBJS)
   -del $(LIB).lib
   $(LB) $(LBOPTS) /OUT:$(LIB).lib @<<
   $(OBJS)
<<

release: all
   copy ..\inc\*.* $(MYTH98ROOT)\inc
   copy $(LIB).lib $(MYTH98ROOT)\lib

clean:
   del *.obj
   del *.err
   del *.lst
   del *.pch
   del *.pdb

# End of makefile 

