$(LIB).lib:   $(OBJS)
    -del $(LIB).lib
   $(LB) $(LBOPTS) /out:$(LIB).lib @<<
   $(OBJS)
<<

release: all
   copy ..\inc\es*.* $(MYTH95ROOT)\inc
   copy $(LIB).lib $(MYTH95ROOT)\lib

clean:
    del *.obj
    del *.res
    del *.err
    del *.lst
    del *.ilk
    del *.pdb
    del *.pch

